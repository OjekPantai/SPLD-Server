{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["routes/**", "config/**", "models/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/media/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
